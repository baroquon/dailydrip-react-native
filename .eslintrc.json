{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "installedESLint": true,
    "rules": {
      "import/no-unresolved": 0,
      "max-len": 0,
      "no-console":0,
      "arrow-body-style": 0,
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
      "semi": ["error", "never"],
      "new-cap": ["error", { "capIsNewExceptions": ["List", "Map"] } ]
    },
    "plugins": [
      "react"
    ]
}
